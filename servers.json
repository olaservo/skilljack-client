{
  "mcpServers": {
    "everything": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-everything@latest"]
    },
    "memory": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory@latest"]
    },
    "skilljack": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@skilljack/mcp@latest", "/path/to/skills"]
    }
  }
}
